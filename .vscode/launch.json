{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Flutter Web (Chrome)",
      "request": "launch",
      "type": "dart",
      "args": [
        "-d",
        "chrome",
        "--web-port=8080",
        "--web-renderer=html"
      ],
      "flutterMode": "debug"
    },
    {
      "name": "Flutter Mobile (Debug)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "debug",
      "args": ["--flavor", "development"]
    },
    {
      "name": "Flutter Profile Mode",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "args": ["--flavor", "production"],
      "toolArgs": ["--dart-define=FLUTTER_APP_ENV=profile"]
    },
    {
      "name": "Flutter Release Mode",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "args": ["--flavor", "production"],
      "toolArgs": ["--dart-define=FLUTTER_APP_ENV=release"]
    }
  ]
}
